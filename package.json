{
  "name": "phosphor-solid",
  "version": "1.1.5",
  "description": "A clean and friendly icon family for Solid-js",
  "author": "rvespula",
  "license": "MIT",
  "repository": "phosphor-solid",
  "homepage": "https://github.com/aguilera51284/phosphor-react",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "solid",
    "solidjs",
    "icons",
    "svg",
    "phosphor",
    "design",
    "interface",
    "UI",
    "UX"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --format amd --transpileOnly",
    "test": "tsdx test --passWithNoTests ",
    "lint": "tsdx lint",
    "prepare": "tsdx build --transpileOnly ",
    "collate": "node bin/collate.js",
    "assemble": "node bin/assemble.js",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,vue}\""
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "babel-preset-solid": "^1.2.4",
    "chalk": "^4.1.2",
    "husky": "^7.0.4",
    "solid-scripts": "0.0.61",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "solid-js": "^1.2.4"
  }
}
