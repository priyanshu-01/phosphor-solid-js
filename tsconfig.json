{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve"
  },
  "baseUrl": "./src",
  "paths": {
    "~/*": ["./src/*"]
  },
  "include": [
    "src"
  ]
}
